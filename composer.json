{
  "name": "tw2113/achoo",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/activitypub": "1.0.1",
    "wpackagist-plugin/autodescription": "4.2.8",
    "wpackagist-plugin/autopost-to-mastodon": "3.7",
    "wpackagist-plugin/blogroll-block": "1.3.3",
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/custom-post-type-ui": "1.14.0",
    "wpackagist-plugin/disable-comments": "2.4.5",
    "wpackagist-plugin/duplicate-post": "4.5",
    "wpackagist-plugin/indieweb": "4.0.1",
    "wpackagist-plugin/post-type-switcher": "3.2.1",
    "wpackagist-plugin/query-monitor": "3.13.1",
    "wpackagist-plugin/reading-time-wp": "2.0.13",
    "wpackagist-plugin/wp-piwik": "1.0.30",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/zero-spam": "5.5.1",
    "wpackagist-theme/octothorpe": "1.1.5"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    },
    "sort-packages": true
  }
}
