{
  "name": "tw2113/achoo",
  "description": "My plugins",
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.10.1",
    "wpackagist-plugin/custom-post-type-ui": "1.13.3",
    "wpackagist-plugin/duplicate-post": "4.5",
    "wpackagist-plugin/disable-comments": "2.4.2",
    "wpackagist-plugin/indieweb": "3.4.7",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/query-monitor": "3.10.1",
    "wpackagist-plugin/reading-time-wp": "2.0.12",
    "wpackagist-plugin/zero-spam": "5.4.4",
    "wpackagist-plugin/wp-piwik": "1.0.27",
    "wpackagist-theme/octothorpe": "1.1.5",
    "wpackagist-plugin/activitypub": "0.14.3",
    "wpackagist-plugin/autopost-to-mastodon": "3.6.6",
    "wpackagist-plugin/post-type-switcher": "3.2.1",
    "wpackagist-plugin/autodescription": "4.2.7.1",
    "wpackagist-plugin/blogroll-block": "1.3.1"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
