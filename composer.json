{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.9.0",
    "wpackagist-plugin/custom-post-type-ui": "1.8.2",
    "wpackagist-plugin/duplicate-post": "4.1.1",
    "wpackagist-plugin/disable-comments": "2.1.0",
    "wpackagist-plugin/indieweb": "3.4.4",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/query-monitor": "3.6.7",
    "wpackagist-plugin/reading-time-wp": "2.0.10",
    "wpackagist-plugin/zero-spam": "5.0.10",
    "wpackagist-plugin/wp-piwik": "1.0.24",
    "wpackagist-theme/octothorpe": "1.1.5",
    "wpackagist-plugin/activitypub": "0.12.0",
    "wpackagist-plugin/autopost-to-mastodon": "3.6.1",
    "wpackagist-plugin/post-type-switcher": "3.2.0",
    "wpackagist-plugin/autodescription": "4.1.3"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
