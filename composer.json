{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/cmb2": "2.7.0",
    "wpackagist-plugin/custom-post-type-ui": "1.8.0",
    "wpackagist-plugin/duplicate-post": "3.2.5",
    "wpackagist-plugin/disable-comments": "1.10.3",
    "wpackagist-plugin/indieweb": "3.4.2",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/query-monitor": "3.6.1",
    "wpackagist-plugin/reading-time-wp": "2.0.10",
    "wpackagist-plugin/zero-spam": "4.9.13",
    "wpackagist-plugin/wp-piwik": "1.0.22",
    "wpackagist-theme/octothorpe": "1.1.5",
    "wpackagist-plugin/activitypub": "0.10.1",
    "wpackagist-plugin/autopost-to-mastodon": "3.6",
    "wpackagist-plugin/post-type-switcher": "3.2.0",
    "wpackagist-plugin/autodescription": "4.1.0"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
