{
  "repositories": [{
    "type": "composer",
    "url": "https://wpackagist.org/"
  }],
  "require": {
    "wpackagist-plugin/classic-editor": "1.4",
    "wpackagist-plugin/cmb2": "2.6.0",
    "wpackagist-plugin/custom-post-type-ui": "1.6.1",
    "wpackagist-plugin/disable-comments": "1.9.0",
    "wpackagist-plugin/indieweb": "3.4.1",
    "wpackagist-plugin/wp-uf2": "1.1.0",
    "wpackagist-plugin/query-monitor": "3.3.2",
    "wpackagist-plugin/reading-time-wp": "2.0.3",
    "wpackagist-plugin/zero-spam": "3.0.6",
    "wpackagist-plugin/wp-piwik": "1.0.19",
    "wpackagist-plugin/wordpress-seo": "9.7",
    "wpackagist-theme/octothorpe": "1.1.5"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugin/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
